{
  "name": "static-js",
  "version": "0.0.1",
  "description": "An AST-based javascript evaluator in a sandboxed environment.",
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "prepare": "husky",
    "build": "tsc -p tsconfig.lib.json",
    "test": "vitest",
    "preextract-api": "pnpm build",
    "extract-api": "api-extractor run --local && rimraf temp"
  },
  "keywords": [],
  "author": "SunsetFi",
  "license": "ISC",
  "packageManager": "pnpm@10.6.2",
  "devDependencies": {
    "@babel/types": "^7.26.10",
    "@microsoft/api-extractor": "^7.52.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.10",
    "@vitest/coverage-v8": "^3.0.9",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "@babel/parser": "^7.26.10",
    "lodash-es": "^4.17.21",
    "type-fest": "^4.37.0"
  }
}
